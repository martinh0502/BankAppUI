@model BankAppUI.Models.NewUser


<div class="card">
    <div class="card-body">
        <form asp-controller="Account" asp-action="Login" method="post">

            <div class="form-group">
                <label asp-for="UserName">Anändarnamn</label>
                <input required class="form-control" type="text" asp-for="UserName" a />
            </div>

            

            <div class="form-group">
                <label asp-for="Password">Lösenord</label>
                <input required class="form-control" type="password" asp-for="Password" />
            </div>

            

            <button class="btn btn-primary" type="submit">Logga in</button>

            <p>@ViewBag.Message</p>





        </form>
    </div>
</div>